    "# посмотрим средние и медианные значения по продажам для самых продаваемых платформ:\n",
    "list_platform_best = ['PS4', 'XOne', 'X360', 'Wii', 'WiiU']\n",
    "\n",
    "platform_best = data.query('platform in @list_platform_best')\n",
    "\n",
    "platform_best_actual = platform_best.query('year_of_release >= 2013')\n",
    "\n",
    "platform_best_pivot2 = platform_best_actual.pivot_table(index='platform',  \n",
