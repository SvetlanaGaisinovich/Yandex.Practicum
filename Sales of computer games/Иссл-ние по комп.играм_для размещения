    "# создаем список платформ:\n",
    "platforms = ['X360', 'PS3', 'WiiU', '3DS']\n",
    "\n",
    "# делаем по этим платформам срез:\n",
    "platforms_other = actual_data.query('platform in @platforms')"
   ]
  },
